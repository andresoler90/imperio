{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "acaronlex/laravel-calendar": "^1.3",
        "bacon/bacon-qr-code": "^2.0",
        "coinbase/coinbase-commerce": "^1.0",
        "fideloper/proxy": "^4.2",
        "haruncpi/laravel-user-activity": "^1.0",
        "huddledigital/zendesk-laravel": "^3.5",
        "joedixon/laravel-translation": "^1.1",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^7.0",
        "laravel/passport": "^9.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.5",
        "laravelcollective/html": "^6.2",
        "lavary/laravel-menu": "^1.7",
        "mercadopago/dx-php": "^2.4",
        "pragmarx/google2fa-laravel": "^1.4",
        "realrashid/sweet-alert": "^3.2",
        "yajra/laravel-datatables-oracle": "^9.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
